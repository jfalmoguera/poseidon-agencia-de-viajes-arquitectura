import { OnDestroy, OnInit, TemplateRef } from '@angular/core';
import { TeleportService } from './teleport.service';
export declare class TeleportDirective implements OnInit, OnDestroy {
    private tpl;
    private service;
    teleportTo: string;
    private viewRef;
    private subscription;
    constructor(tpl: TemplateRef<any>, service: TeleportService);
    ngOnInit(): void;
    ngOnDestroy(): void;
}
export declare class TeleportModule {
}
